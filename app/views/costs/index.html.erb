<p id="notice"><%= notice %></p>

<h1>Listing Costs</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Material</th>
      <th>Product</th>
      <th>Pressure</th>
      <th>Size</th>
      <th>Quantity</th>
      <th>Hvk</th>
      <th>Fujii</th>
      <th>Imamoto</th>
      <th>Selling price</th>
      <th>Remarks</th>
      <th>Sign</th>
      <th>Product model</th>
      <th>Unit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @costs.each do |cost| %>
      <tr>
        <td><%= cost.subject %></td>
        <td><%= cost.material %></td>
        <td><%= cost.product %></td>
        <td><%= cost.pressure %></td>
        <td><%= cost.size %></td>
        <td><%= cost.quantity %></td>
        <td><%= cost.hvk %></td>
        <td><%= cost.fujii %></td>
        <td><%= cost.imamoto %></td>
        <td><%= cost.selling_price %></td>
        <td><%= cost.remarks %></td>
        <td><%= cost.sign %></td>
        <td><%= cost.product_model %></td>
        <td><%= cost.unit %></td>
        <td><%= link_to 'Show', cost %></td>
        <td><%= link_to 'Edit', edit_cost_path(cost) %></td>
        <td><%= link_to 'Destroy', cost, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cost', new_cost_path %>
